				"variable.language"
			],
			"settings": {
				"foreground": "#E5C07B"
			}
		},
		{
			"name": "variables",
			"scope": [
				"support.type.property-name",
				"support.variable",
				"variable"
			],
			"settings": {
				"foreground": "#D4D7D9"
			}
		},
